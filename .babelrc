{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      "syntax-dynamic-import",
      "@babel/plugin-proposal-object-rest-spread",
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ]
    ],
    "env": {
      "test": {
          "presets": [["env", {"targets": {"node": "current"}}]]
      }
    }
  }